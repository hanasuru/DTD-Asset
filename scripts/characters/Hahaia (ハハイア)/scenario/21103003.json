        "command_option": null,
        "command_time": null,
        "resorce_id": 110300,
        "text_option": {
            "character_name": "ハハイア",
            "character_text": "いやいや、だ、大丈夫だとも……はあはあ……\nこれもまたひとつの重大な経験だからねえ……",
            "voice": "110300_scenario_000_001_009",
