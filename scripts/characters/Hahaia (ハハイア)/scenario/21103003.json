    },
    {
        "command_id": 26,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 110300,
        "text_option": {
            "character_name": "ハハイア",
            "character_text": "灼熱の棒が刺さったような、などとも形容される痛みだが、\n確かに頷けるというものだよ……うっくうっ……",
            "voice": "110300_scenario_000_001_008",
