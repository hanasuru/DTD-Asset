    },
    {
        "command_id": 28,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 110300,
        "text_option": {
            "character_name": "ハハイア",
            "character_text": "っく……ふうっ……ううっ……\nまあ、気にせずに動いてくれたまえよ……は……はあっ……",
            "voice": "110300_scenario_000_001_010",
            "voice_option": null
