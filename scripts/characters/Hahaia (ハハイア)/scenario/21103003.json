        "command_option": null,
        "command_time": null,
        "resorce_id": 110300,
        "text_option": {
            "character_name": "ハハイア",
            "character_text": "うっ……くぅんっ……この、痛み……うううっ……！",
            "voice": "110300_scenario_000_001_007",
