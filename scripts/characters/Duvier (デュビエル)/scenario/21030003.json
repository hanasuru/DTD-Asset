        "command_option": null,
        "command_time": null,
        "resorce_id": 103000,
        "text_option": {
            "character_name": "デュビエル",
            "character_text": "やっぱり酔っ払ってるって？　おう、酔っ払ってるねえ！\n私は酔っ払ってるのを否定しない酔っ払いなんでねえ",
            "voice": "103000_scenario_000_001_005",
