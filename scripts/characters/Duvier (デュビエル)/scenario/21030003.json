    },
    {
        "command_id": 26,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 103000,
        "text_option": {
            "character_name": "デュビエル",
            "character_text": "なあマスター、もっといいモン挟みたくないか？\nそうそう、それだよそれオ・チ・ン・ポ♪",
            "voice": "103000_scenario_000_001_010",
            "voice_option": null
        }
    },
    {
        "command_id": 27,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 103000,
        "text_option": {
            "character_name": "デュビエル",
            "character_text": "それを私のお尻に擦りつけてくれよ、さあさあ、早く早くぅ♪",
            "voice": "103000_scenario_000_001_011",
            "voice_option": null
