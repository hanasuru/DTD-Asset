        "command_option": null,
        "command_time": null,
        "resorce_id": 107100,
        "text_option": {
            "character_name": "ザアピエル",
            "character_text": "御主はじわじわ動いて、我と御主のまぐわいを\n潤滑にしようとしておるな……",
