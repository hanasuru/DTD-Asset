    {
        "command_id": 36,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 107100,
        "text_option": {
            "character_name": "ザアピエル",
            "character_text": "はあっはあっ……んっんっ……\n我が気を紛らわそうと喋りまくっている間にも……",
