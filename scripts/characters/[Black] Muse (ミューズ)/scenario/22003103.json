        "command_time": null,
        "resorce_id": 200310,
        "text_option": {
            "character_name": "ブラック・ミューズ",
            "character_text": "んくぅ……！",
            "voice": "200310_scenario_000_001_009",
            "voice_option": null
        }
    },
    {
        "command_id": 31,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 200310,
        "text_option": {
            "character_name": "ブラック・ミューズ",
            "character_text": "あっ、ばかっ……慣らす必要ないって、\nいったのに……うっくぅ……ううん……！",
            "voice": "200310_scenario_000_001_010",
            "voice_option": null
        }
    },
    {
        "command_id": 32,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 200310,
        "text_option": {
            "character_name": "ブラック・ミューズ",
            "character_text": "指一本でそんなに苦しそうなのに無理するなって？",
            "voice": "200310_scenario_000_001_011",
            "voice_option": null
        }
    },
    {
        "command_id": 33,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 200310,
        "text_option": {
            "character_name": "ブラック・ミューズ",
            "character_text": "な、なによ！　へ、平気だっていってるでしょ！？\nはっあぅっ……くうんっ……！",
            "voice": "200310_scenario_000_001_012",
            "voice_option": null
        }
    },
    {
        "command_id": 34,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 200310,
        "text_option": {
            "character_name": "ブラック・ミューズ",
            "character_text": "あ、あたしが平気だっていってるんだから、\nさ、さっさとそのデカチンポを挿れなさいよ！　ほら！　早く！",
            "voice": "200310_scenario_000_001_013",
            "voice_option": null
        }
    },
