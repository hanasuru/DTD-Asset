        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 200310,
        "text_option": {
            "character_name": "ブラック・ミューズ",
            "character_text": "あっああっあんたっ！？　あたしの子宮っをっ、\nケツマンコの方からごちゅごちゅしてるわねっ！？",
