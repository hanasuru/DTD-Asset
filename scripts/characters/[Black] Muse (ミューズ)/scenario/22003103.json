    },
    {
        "command_id": 54,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 200310,
        "text_option": {
            "character_name": "ブラック・ミューズ",
            "character_text": "あんたはあたしのケツマンコで大興奮してくれないと困るのよ！！\n早く！　早く！　動いて！！！",
            "voice": "200310_scenario_000_001_023",
            "voice_option": null
