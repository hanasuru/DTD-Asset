        "command_time": null,
        "resorce_id": 200310,
        "text_option": {
            "character_name": "ブラック・ミューズ",
            "character_text": "うっくぅんっ……苦しくなんてないわよ……",
            "voice": "200310_scenario_000_001_016",
            "voice_option": null
        }
    },
    {
        "command_id": 45,
        "command": "Text",
        "command_option": null,
        "command_time": null,
        "resorce_id": 200310,
        "text_option": {
            "character_name": "ブラック・ミューズ",
            "character_text": "このまま、あんたを、あたしのケツマンコで、\nんぐっ……メロメロにするんだから……！",
