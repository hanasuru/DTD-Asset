        "resorce_id": 200310,
        "text_option": {
            "character_name": "ブラック・ミューズ",
            "character_text": "そっそんなテクニックどこで覚えてきたのよっ\n腹立つっあっああっああんっふあっあああんっ！！",
            "voice": "200310_scenario_000_001_027",
