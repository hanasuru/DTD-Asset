    },
    {
        "command_id": 38,
        "command": "Text",
        "command_option": "",
        "command_time": "",
        "resorce_id": 101200,
        "text_option": {
            "character_name": "クァディシン・ポルタ",
            "character_text": "においが強くてっじゅぶぶぶっ……先走りも濃くて\nおいひーよおぉおぢゅぶっ……ぢゅるうっじゅぶぶっ",
