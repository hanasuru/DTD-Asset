    },
    {
        "command_id": 36,
        "command": "Text",
        "command_option": "",
        "command_time": "",
        "resorce_id": 101200,
        "text_option": {
            "character_name": "クァディシン・ポルタ",
            "character_text": "くふぅぅんっ……んっふううっ……このまええっちして\nふぇらずっとこうひてまふふぁーっちのオチンポ考えながらっ",
            "voice": "101200_scenario_000_002_009",
            "voice_option": null
        }
    },
    {
        "command_id": 37,
        "command": "Text",
        "command_option": "",
        "command_time": "",
        "resorce_id": 101200,
        "text_option": {
            "character_name": "クァディシン・ポルタ",
            "character_text": "ひとりでしてふぁんふぁよふぇっじゅるるっ……\nんっんふうっ想像よりもずっとんぢゅうぶぶっ",
            "voice": "101200_scenario_000_002_010",
            "voice_option": null
