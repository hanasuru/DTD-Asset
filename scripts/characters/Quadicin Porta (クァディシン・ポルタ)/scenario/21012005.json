        "command_time": "",
        "resorce_id": 101200,
        "text_option": {
            "character_name": "クァディシン・ポルタ",
            "character_text": "指もとまんなくなっひゃうよおっじゅぶっじゅるっ……\nんっんぢゅるうっ……",
            "voice": "101200_scenario_000_002_012",
