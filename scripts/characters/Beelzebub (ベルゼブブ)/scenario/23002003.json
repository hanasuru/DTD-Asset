        "command_time": null,
        "resorce_id": 300200,
        "text_option": {
            "character_name": "ベルゼブブ",
            "character_text": "今のは聞かなかったことにしてください。\nそのデカチンを前にしてテンパった上の妄言です",
            "voice": "300200_scenario_000_001_006",
            "voice_option": null
